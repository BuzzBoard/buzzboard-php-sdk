{
    "name": "BuzzBoard/buzzboard-php-sdk",
    "type": "library",
    "description": "BuzzBoard PHP SDK",
    "keywords": ["BuzzBoard", "BuzzBoard SDK"],
    "homepage": "https://github.com/BuzzBoard/buzzboard-php-sdk",
    "authors": [
        {
            "name": "Mir Adnan",
            "email": "adnan@vsplash.net",
            "homepage": "https://www.buzzboard.com",
            "role": "Team Lead Development"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "*",
        "ext-mbstring": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.21"
    },
    "autoload": {
        "psr-4": {
            "BuzzBoard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Buzzboard\\Tests\\": "tests/"
        }
    }
}
